<Window x:Class="AppDebitiV2.Views.AddRichiesta"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppDebitiV2.Views"
        mc:Ignorable="d"
        xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:UC="clr-namespace:AppDebitiV2.Views.UC"
        Title="AddRichiesta" Height="350" Width="560" Style="{StaticResource WindowBase}">
    <Grid>
        
        <Border Background="White" VerticalAlignment="Center" HorizontalAlignment="Center" Width="520" Height="250" Style="{StaticResource MyBorder}">


            <Border.Effect>
                <DropShadowEffect BlurRadius="30" Color="#ccdaf0" ShadowDepth="3"/>
            </Border.Effect>
            <Grid Margin="10">


                <Button VerticalAlignment="Bottom" HorizontalAlignment="Right" Content="new page" Click="Button_Click" Margin="0,0,5,5"/>
                <Frame x:Name="PageControler"  NavigationUIVisibility="Hidden" />
            </Grid>

        </Border>


    </Grid>
</Window>
