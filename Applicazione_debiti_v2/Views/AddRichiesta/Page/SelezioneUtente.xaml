                <UC:UserBox Width="160" Height="50" Style="{StaticResource UserBoxBorder}" />
                <UC:UserBox Width="160" Height="50" Style="{StaticResource UserBoxBorder}" />
                <UC:UserBox Width="160" Height="50" Style="{StaticResource UserBoxBorder}" />
